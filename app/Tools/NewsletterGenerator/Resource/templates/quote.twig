<!-- ####### QUOTE ####### -->
{% set data = data[0] %}
<table class="quote" width="100%" bgcolor="#c46f17" border="0" cellpadding="0" cellspacing="0" style="color: #7c7c7c; font-family:  Arial, Helvetica, sans-serif, Latha, Inaimathi; font-size: 68.8%; cursor: default;">
    <tr>
        <td class="quote-open-image" align="right" valign="top" width="40" style="padding-top: 10px; color: white; font-family:  Arial, Helvetica, sans-serif, Latha, Inaimathi; cursor: default; font-size: 45px; font-weight: bold;">
            â€œ
        </td>
        <td class="quote-text" style="padding: 26px 20px 15px 15px; font-size: 130%; font-weight: normal; line-height: 1.6em; color: #7c7c7c; font-family:  Arial, Helvetica, sans-serif, Latha, Inaimathi; cursor: default;" width="610" valign="bottom">
            <a style="color: white; text-decoration: none;" href="{{data['url']}}{% if ('?' in data['url']) == true  %}&amp;{% else %}?{% endif %}{% include 'utm_param.twig' %}" target="_blank" title="{{data['title']}}">
                <span class="text">{{data['blurb']}}</span>
            </a>
            <span class="author_signature" style="display: block; font-size: 135%; color: white; font-weight: bold; text-align: right; padding-top: 5px">
                &ndash; <a style="color: white; text-decoration: none;" href="{{data['url']}}{% if ('?' in data['url']) == true %}&amp;{% else %}?{% endif %}{% include 'utm_param.twig' %}" title="{{data['title']}}" target="_blank">{{config['quote']['signature']}}</a>
            </span>
        </td>
    </tr>
</table>
<div class="block_shadow" style="height: 4px; background-color: #CCC7B0; margin-left: 4px;"></div>